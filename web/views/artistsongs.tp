<h1><%= title %></h1>
<table>
<tr>
<th>Song</th> <th>Album</th>
</tr>
<% for( var i = 0; i < songs.length ; i++ ) { %>
<tr>
<td>
    <a href="/song/<%= songs[i].hash %>"><%= songs[i].title %></a>
</td>
<td>
    <a href="/album/<%= songs[i].albumHash %>"><%= songs[i].album %></a></li>
</td>
</tr>
<% } %>
</table>
